    "#libraries\n",
    "import sys\n",
    "!conda install --yes --prefix {sys.prefix} requests\n",
    "!conda install --yes --prefix {sys.prefix} lxml\n",
    "!conda install --yes --prefix {sys.prefix} pandas"
